<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Page One</title>
</head>
<body>
  <h1>BREAD (Browse, Read, Edit, Add, Delete)</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>String</th>
        <th>Integer</th>
        <th>Float</th>
        <th>Date</th>
        <th>Boolean</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach((item, index) => {%>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= item.string %></td>
        <td><%= item.integer %></td>
        <td><%= item.float %></td>
        <td><%= item.date %></td>
        <td><%= item.boolean %></td>
        <td><%= item.action %></td>
      </tr>
      <% })%>
    </tbody>
  </table>
  <br>
  <a href="/add">Add</a>
</body>
</html>